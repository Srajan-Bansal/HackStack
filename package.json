{
	"name": "HackStack",
	"private": true,
	"scripts": {
		"build": "turbo build",
		"dev": "turbo dev",
		"lint": "turbo lint",
		"lint:ci": "turbo lint:ci",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"web": "turbo run dev --filter=!submission-webhook",
		"prisma:generate": "pnpm --filter @repo/db exec pnpm prisma generate"
	},
	"devDependencies": {
		"prettier": "^3.2.5",
		"turbo": "^2.3.3",
		"typescript": "5.5.4"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=18"
	}
}
